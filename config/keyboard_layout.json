{
  "title": "Default Piano Keyboard Layout",
  "description": "Maps keyboard keys to musical notes",
  "basetone": "C",
  "key_mappings": {
    "a": ".1", "w": ".#1",
    "s": ".2", "e": ".#2",
    "d": ".3", "r": ".4", "t": ".#4",
    "f": ".5", "v": ".#5",
    "g": ".6", "b": ".#6", "y": ".7",
    "h": "1", "u": "#1",
    "j": "2", "i": "#2",
    "k": "3", "o": "4", ",": "#4",
    "l": "5", "p": "#5",
    ";": "6", "/": "#6",
    "[": "7",
    "'": "^1",
    "]": "^2",
    "\\": "^3"
  },
  "controls": {
    "space": "stop",
    "esc": "quit",
    "1": "change_basetone",
    "2": "change_instrument",
    "3": "change_layout",
    "+": "volume_up",
    "=": "volume_up",
    "-": "volume_down",
    "_": "volume_down"
  }
}