{
  "title": "A as Mi Piano Keyboard Layout",
  "description": "Maps keyboard keys to musical notes",
  "basetone": "C",
  "key_mappings": {
    "a": ".3", "w": ".4", "z": ".#4",
    "s": ".5", "e": ".#5",
    "d": ".6", "r": ".7", "c": ".#6",
    "f": "1", "t": "#1",
    "g": "2", "y": "#2",
    "h": "3", "u": "4", "n": "#4",
    "j": "5", "i": "#5",
    "k": "6", "o": "7", ",": "#6",
    "l": "^1", "p": "^#1",
    ";": "^2", "/": "^4", "[": "^#2",
    "'": "^3",
    "]": "^5",
    "\\": "^6"
  },
  "controls": {
    "space": "stop",
    "esc": "quit",
    "1": "change_basetone",
    "2": "change_instrument",
    "3": "change_layout"
  }
}