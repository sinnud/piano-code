{
  "title": "La Based Piano Keyboard Layout",
  "description": "Maps keyboard keys to musical notes with 'a' as low la (.6)",
  "basetone": "C",
  "key_mappings": {
    "q": ".5",
    "a": ".6", "w": ".7",
    "s": "1", "z": ".#6", "e": "#1",
    "d": "2", "r": "#2",
    "f": "3", "t": "4", "v": "#4",
    "g": "5", "y": "#5",
    "h": "6", "u": "7",
    "j": "^1", "n": "#6", "i": "^#1",
    "k": "^2", "o": "^#2",
    "l": "^3", "p": "^4", ".": "^#4",
    ";": "^5", "[": "^#5",
    "'": "^6",
    "]": "^7"
  },
  "controls": {
    "space": "stop",
    "esc": "quit",
    "1": "change_basetone",
    "2": "change_instrument",
    "3": "change_layout",
    "+": "volume_up",
    "=": "volume_up",
    "-": "volume_down",
    "_": "volume_down"
  }
}